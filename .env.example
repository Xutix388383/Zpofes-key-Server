# Discord webhook for logging key verification attempts
WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_id/your_webhook_token

# Optional: override default Express port (default is 3000)
PORT=8080

# Optional: enable verbose logging (true/false)
VERBOSE=true

# Optional: restrict access to specific IPs (comma-separated)
ALLOWED_IPS=127.0.0.1,192.168.1.100

# Optional: toggle HWID binding enforcement
ENFORCE_HWID=true
